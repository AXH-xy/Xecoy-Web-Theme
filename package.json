{
  "name": "xecoy-theme",
  "displayName": "Xecoy Theme",
  "description": "An intricately designed theme catering to web development, providing enhanced highlighting for a polished coding experience",
  "author": {
    "name": "AXH-xecoy"
  },
  "license":"MIT",
  "version": "1.0.2",
  "engines": {
    "vscode": "*"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Xecoy Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-color-theme.json"
      },
      {
        "label": "Xecoy Light",
        "uiTheme": "vs",
        "path": "./themes/light-color-theme.json"
      }
    ],
    "languages": [
      {
        "id": "configuration",
        "extensions": [
          ".cfg",
          ".lang"
        ]
      },
      {
        "id": "log",
        "extensions": [
          ".log"
        ]
      },
      {
        "id": "ini",
        "extensions": [
          ".ini"
        ]
      },
      {
        "id": "glsl",
        "extensions": [
          ".glsl",
          ".fsh",
          ".vsh",
          ".inc"
        ]
      }
    ],
    "grammars": [
      {
        "language": "configuration",
        "scopeName": "source.cfg",
        "path": "./syntaxes/configuration.tmGrammar.json"
      },
      {
        "language": "ini",
        "scopeName": "source.ini",
        "path": "./syntaxes/ini.tmGrammar.json"
      },
      {
        "language": "log",
        "scopeName": "source.log",
        "path": "./syntaxes/log.tmGrammar.json"
      },
      {
        "language": "glsl",
        "scopeName": "source.glsl",
        "path": "./syntaxes/glsl.tmGrammar.json"
      },
      {
        "path": "./syntaxes/php.tmGrammar.json",
        "scopeName": "source.injection.php",
        "injectTo": ["text.html.php"]
      },
      {
        "path": "./syntaxes/csharp.tmGrammar.json",
        "scopeName": "source.injection.cs",
        "injectTo": ["source.cs"]
      },
      {
        "path": "./syntaxes/rust.tmGrammar.json",
        "scopeName": "source.injection.rust",
        "injectTo": ["source.rust"]
      },
      {
        "path": "./syntaxes/rust.no-enum.tmGrammar.json",
        "scopeName": "source.injection.no-enum.rust",
        "injectTo": ["source.rust"]
      },
      {
        "path": "./syntaxes/python.tmGrammar.json",
        "scopeName": "source.injection.python",
        "injectTo": ["source.python"]
      },
      {
        "path": "./syntaxes/log.no-string.tmGrammar.json",
        "scopeName": "source.injection.log",
        "injectTo": ["source.log"]
      },
      {
        "path": "./syntaxes/log.no-block.tmGrammar.json",
        "scopeName": "source.injection.no-block.log",
        "injectTo": ["source.log"]
      }
    ]
  },
  "repository": {
    "url": "https://github.com/Xecoy-Studio/Xecoy-Web-Theme"
  },
  "icon": "icon.png",
  "publisher": "xecoy-studio"
}