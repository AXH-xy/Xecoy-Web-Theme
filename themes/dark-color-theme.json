	"tokenColors": [
		{
			"name": "注释",
			"scope":"comment",
			"settings": {
				"foreground": "#707070",
				"fontStyle": ""
			}
		},
		{
			"name": "常量值",
			"scope": "constant",
			"settings": {
				"foreground": "#60b7b7",
				"fontStyle": ""
			}
		},
		{
			"name": "关键字",
			"scope":"keyword",
			"settings": {
				"foreground": "#cf9977",
				"fontStyle": ""
			}
		},
		{
			"name": "无效内容",
			"scope":"invalid",
			"settings": {
				"foreground": "#DA5050",
				"fontStyle": ""
			}
		},
		{
			"name": "标记文本",
			"scope":"markup",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "常规数据",
			"scope":"meta",
			"settings": {
				"foreground": "#CCCCCC",
				"fontStyle": ""
			}
		},
		{
			"name": "结构与对象",
			"scope":"storage",
			"settings": {
				"foreground": "#cf9977",
				"fontStyle": ""
			}
		},
		{
			"name": "字符串",
			"scope": "string",
			"settings": {
				"foreground": "#6AAA70",
				"fontStyle": ""
			}
		},
		{
			"name": "特殊",
			"scope": "support",
			"settings": {
				"foreground": "#cf9977",
				"fontStyle": ""
			}
		},
		//Web支持与基本色
		{
			"scope":[
				"keyword.operator", //操作符样式
