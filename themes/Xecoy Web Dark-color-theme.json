	"tokenColors": [
		{
			"name": "注释",
			"scope":"comment",
			"settings": {
				"foreground": "#707070",
				"fontStyle": ""
			}
		},
		{
			"name": "常量值",
			"scope": "constant",
			"settings": {
				"foreground": "#60b7b7",
				"fontStyle": ""
			}
		},
		{
			"name": "关键字",
			"scope":"keyword",
			"settings": {
				"foreground": "#cf9977",
				"fontStyle": ""
			}
		},
		{
			"name": "非法字符",
			"scope":"invalid",
			"settings": {
				"foreground": "#DA5050",
				"fontStyle": ""
			}
		},
		{
			"name": "标记文本",
			"scope":"markup",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "常规数据",
			"scope":"meta",
			"settings": {
				"foreground": "#CCCCCC",
				"fontStyle": ""
			}
		},
		{
			"name": "结构与对象",
			"scope":"storage",
			"settings": {
				"foreground": "#cf9977",
				"fontStyle": "italic"
			}
		},
		{
			"name": "字符串",
			"scope": "string",
			"settings": {
				"foreground": "#6AAA70",
				"fontStyle": ""
			}
		},
		{
			"name": "框架",
			"scope": "support",
			"settings": {
				"foreground": "#cf9977",
				"fontStyle": ""
			}
		},
		//联合调色
		{
			"scope":[
				"keyword.operator", //操作符样式
				"variable.parameter.keyframe-list", //@Keyframes名称颜色
				"punctuation.definition.entity" //CSS选择器符号样式
			],
			"settings": {
				"foreground": "#CCCCCC",
				"fontStyle": ""
			}
		},
		{
			"scope":[
				"meta.function-call entity.name.function", //以调用形式的函数名样式
				"source support.function", //css方法
				"entity.other.keyframe-offset", //CSS form to样式
				"keyword.operator.logical" //CSS and样式
			],
			"settings": {
				"foreground": "#CCCCCC",
				"fontStyle": "italic"
			}
		},
		{
			"scope":[
				"meta.function.definition entity.name.function", //声明函数的函数名样式
				"meta.definition.function", //声明函数的函数名样式
				"meta.definition.method", //声明方法的函数名样式
				"keyword.control.at-rule", //CSS@规则样式
				"support.constant" //CSS常量名如screen
			],
			"settings": {
				"foreground": "#C985DB",
				"fontStyle": ""
			}
		},
		{
			"scope":[
				"constant.character.escape", //转义字符样式
				"constant.other.character-class.regexp", //正则表达式转义字符样式
				"keyword.control.anchor.regexp", //正则表达式关键转义字符颜色
				"constant.other.color", //CSS颜色字符样式
				"support.constant.color" //CSS颜色字符样式
			],
			"settings": {
				"foreground": "#D7BA7D",
				"fontStyle": ""
			}
		},
		{
			"scope":[
				"entity.name.tag", //html标签样式
				"meta.definition.variable", //声明变量名称样式
				"meta.definition.variable variable.other.constant", //声明常量时的变量名称样式
				"variable.other.property", //调用对象属性名称样式
				"variable.object.property", //类属性名称样式
				"variable.parameter", //函数参数名样式
				"meta.selector entity.other.attribute-name", //CSS中的其他选择器样式
				"meta.selector entity.name.tag" //CSS中的元素选择器样式
			],
			"settings": {
				"foreground": "#55A0D0",
				"fontStyle": ""
			}
		},
		{
			"scope":[
				"variable.other.constant.property", //调用对象常量属性名称样式
				"variable.other.enummember" //枚举类对象名称样式
			],
			"settings": {
				"foreground": "#55A0D0",
				"fontStyle": "italic"
			}
		},
		{
			"scope":[
				"keyword.operator.new", //new关键字样式
				"variable.language.this", //this关键字样式
				"variable.language.super", //super关键字样式
				"keyword.operator.expression", //of,instanceof,in等关键字
				"entity.other.attribute-name", //html元素属性名称样式
				"keyword.operator.quantifier.regexp", //正则表达式控制出现频率{0,?}
				"string.regexp punctuation.definition.string", //正则表达式斜线颜色
				"keyword.other" //正则表达式匹配类型颜色
			],
			"settings": {
				"foreground": "#cf9977",
				"fontStyle": ""
			}
		},
		{
			"scope":[
				"constant.language", //json boolean & null 颜色
				"constant.language.boolean.true", //true值 颜色
				"constant.language.boolean.false", //false值 颜色
				"constant.language.null", //null常量样式
				"variable.argument.css", //css变量名称样式
				"variable.css" //css变量名称样式
			],
			"settings": {
				"foreground": "#cf9977",
				"fontStyle": "italic"
			}
		},
		{
			"scope":[
				"punctuation.definition.tag.begin", //html括号样式
				"punctuation.definition.tag.end"
			],
			"settings": {
				"foreground": "#A0A0A0",
				"fontStyle": ""
			}
		},
		{
			"scope":[
				"keyword.other.unit", //CSS百分比px等符号样式
				"punctuation.definition.group.regexp", //正则表达式圆括号样式
				"meta.group.regexp", //正则表达式圆括号组结构
				"keyword.operator.or.regexp" //正则表达式或运算符
			],
			"settings": {
				"foreground": "#60b7b7",
				"fontStyle": ""
			}
		},
		{
			"scope":[
				"support.constant.property-value" //CSS属性值样式
			],
			"settings": {
				"foreground": "#6AAA70",
				"fontStyle": ""
			}
		},
		{
			"scope":[
				"entity.other.attribute-name.pseudo-class", //伪元素选择器
				"entity.other.attribute-name.pseudo-element", //伪类选择器
				"meta.assertion", //正则表达式预查语句关键字颜色
				"meta.group.assertion.regexp punctuation.definition.group.regexp", //正则表达式断言括号颜色
				"meta.group.assertion.regexp keyword.operator.or.regexp", //正则表达式断言或运算符
				"meta.group.assertion.regexp", //正则表达式断言结构颜色
				"meta.template.expression", //插值表达式
				"expression.embedded.vue" //Vue插值表达式
			],
			"settings": {
				"foreground": "#cf7777",
				"fontStyle": ""
			}
		}
